{
  "expo": {
    "name": "FestivApp",
    "slug": "festivApp",
    "scheme": "festivapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/oficial_festiapp.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/oficial_festiapp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.festivapp.app",
      "supportsTablet": true,
      "projectId": "7a9acb99-f924-4fd3-81f3-20b74d328d16"
    },
    "android": {
      "package": "festiv_app.mb_25",
      "adaptiveIcon": {
        "foregroundImage": "./assets/oficial_festiapp.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "festivapp",
              "host": "*.festivapp.com",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "projectId": "7a9acb99-f924-4fd3-81f3-20b74d328d16",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/oficial_festiapp.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/oficial_festiapp.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7a9acb99-f924-4fd3-81f3-20b74d328d16"
      }
    },
    "owner": "brayanbj27"
  }
}
